---
import Layout from '../layouts/Layout.astro';
import TablaHorario from '../components/TablaHorario.astro'

const title = "Horario";
---
<style>
  html{
    background:white;
  }
  table {
    border:10px;
    color: black;
    display:grid;
    grid-template-columns: 12,5%;
  }
  td{
    margin: 10px;
  }
</style>
<Layout title={title}>
  <TablaHorario />
</Layout>